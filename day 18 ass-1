#include <stdio.h>

int main() {
    int n, i;
    int num, lowest;

    printf("Enter the total number of elements: ");
    scanf("%d", &n);

    printf("Enter number 1: ");
    scanf("%d", &lowest);

    for (i = 2; i <= n; i++) {
    
        printf("Enter number %d: ", i);
        scanf("%d", &num);

        if (num < lowest) {
            lowest = num;
        }
    }
    printf("The lowest number is %d", lowest);

    return 0;
}
